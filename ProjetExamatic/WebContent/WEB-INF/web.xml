  </servlet>
  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StudentHub</servlet-name>
    <servlet-class>fr.examatic.student.StudentHub</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentHub</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TeacherHub</servlet-name>
    <servlet-class>fr.examatic.student.TeacherHub</servlet-class>
  </servlet>
